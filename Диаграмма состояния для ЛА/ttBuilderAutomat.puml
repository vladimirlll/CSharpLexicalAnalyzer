@startuml "Transition table builder"

[*] -up-> S1 : [0-9]
S1 --> S1 : [0-9]
S1 --> S2 : WS
S2 : Введено\nначальное\nсостояние
S2 -up-> S3 : -
S3 -up-> S4 : >
S4 -right-> S5 : WS
S5 --> S1 : [0-9]
S5 -right-> S11 : *
S11 -right-> S12 : WS 
S12 : Введены\nвсе\nклассы\nсимволов


S1 -right-> S7 : Запятая
S7 : Введен\nсимвольный\nкласс
S7 -left-> S1 : Пробел

S1 --> S8 : =
S8 --> S9 : >
S9 -right-> S6 : Space
S6 --> S6 : [0-9\-]
S6 --> S10 : \\r
S10 : Введено\nконечное\nсостояние
S10 --> [*] : EOL

@enduml 